{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "osc99": true,
  "palette": {
    "windows-blue": "#26C6DA",
    "git-foreground": "#193549",
    "git-background": "white",
    "git-modified": "#DCDBA8",
    "git-diverged": "#FAA381",
    "git-ahead": "#C9DBBA",
    "git-behind": "#F5CDA7",
    "node-green": "#6CA35E",
    "path-foreground": "white",
    "path-background": "#757575",
    "text": "#706C61",
    "gray": "#757575",
    "mint-green": "#ADFC92",
    "red": "red"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " \uE77F ",
            "postfix": "<p:mint-green,>\u2022</>"
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "red",
          "properties": {
            "prefix": " \uE738 ",
            "postfix": "<p:mint-green,>\u2022</>"
          }
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " <p:red,>\uE753</> ",
            "postfix": " <p:mint-green,>\u2022</>"
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " <p:node-green,>\uE718</> ",
            "postfix": " <p:mint-green,>\u2022</>"
          }
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " [ ",
            "postfix": " <p:mint-green,>\u2022</> ",
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "time_format": "15<p:mint-green,>:</>04<p:mint-green,>:</>05",
            "postfix": " ] "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "╭─\u27E8 ",
            "template": "{{ .Env.USERNAME }} <p:mint-green,>\u2022</> {{ .Env.COMPUTERNAME }}",
            "postfix": ""
          }
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " <p:mint-green,>\u2022</> ",
            "macos": "mac",
            "postfix": " \u27E9 "
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "<p:mint-green,>\u27A6</> ",
            "style": "agnoster_short",
            "max_depth": "2",
            "home_icon": "\uf7db "
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:text",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "properties": {
            "prefix": "<p:mint-green,transparent>\u27A6 </>",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "╰─"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:text",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\uE23A"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "text": "><p:mint-green,>$</>",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<p:gray,>></> "
  }
}
