{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "osc99": true,
  "palette": {
    "windows-blue": "#26C6DA",
    "git-foreground": "#193549",
    "git-background": "white",
    "git-modified": "#DCDBA8",
    "git-diverged": "#FAA381",
    "git-ahead": "#C9DBBA",
    "git-behind": "#F5CDA7",
    "node-green": "#6CA35E",
    "text": "#706C61",
    "error": "#B6465F",
    "gray": "#ADA0A6",
    "mint-green": "#ADFC92",
    "red": "red"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "╭─\u27E8 ",
            "template": "{{ .Env.USERNAME }} <p:mint-green,>\u2022</> {{ .Env.COMPUTERNAME }} \u27E9 ",
            "postfix": ""
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "<p:mint-green,>\u2022</> ",
            "style": "agnoster_short",
            "max_depth": 2,
            "home_icon": "\uf7db "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " [ ",
            "postfix": " <p:mint-green,>\u2022</>",
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          }
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "postfix": " <p:mint-green,>\u2022 </>",
            "macos": "mac"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "time_format": "15<p:mint-green,>:</>04<p:mint-green,>:</>05",
            "postfix": " ] "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:text",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "properties": {
            "prefix": " ",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": " <p:mint-green,>{{ .UpstreamIcon }}</>{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " \uE77F "
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "red",
          "properties": {
            "prefix": " \uE738 "
          }
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " <p:red,>\uE753</> "
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": " <p:node-green,>\uE718</> "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "╰─"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:text",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:error{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "prefix": "",
            "template": "{{if gt .Code 0}} \uE23A: {{ .Code }}{{ end }}",
            "postfix": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "text": "\u276D",
            "postfix": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "prefix": "",
            "text": "<p:mint-green,>$</>",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:text",
    "template": "\u276D"
  }
}